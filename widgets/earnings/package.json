{
  "name": "wealthica-earnings-widget",
  "description": "Wealthica Earnings Widget",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf public/assets; rollup -c && mkdir -p ../../public/mani-coder/wealthica-portfolio-addon/widgets/earnings/ && rm -rf ../../public/mani-coder/wealthica-portfolio-addon/widgets/pnl/* && cp -vR public/* ../../public/mani-coder/wealthica-portfolio-addon/widgets/earnings/",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "deploy": "npm run build && firebase deploy --only hosting:mani-coder"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/date-fns": "^2.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@wealthica/wealthica.js": "0.0.16",
    "autoprefixer": "^10.2.5",
    "clsx": "^1.1.1",
    "date-fns": "^2.21.3",
    "postcss": "^8.2.15",
    "sirv-cli": "^1.0.0",
    "tailwindcss": "^2.1.2"
  }
}
